{
  "name": "backend_postgres",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "start:dev": "ENV=DEVELOPMENT node dist/app.js",
    "start:prod": "ENV=PRODUCTION node dist/app.js",
    "watch:dev": "ENV=DEVELOPMENT tsx watch --inspect=0.0.0.0:9229 src/app.ts",
    "watch:prod": "ENV=PRODUCTION tsx watch --inspect=0.0.0.0:9229 src/app.ts",
    "prisma:generate": "npx prisma generate",
    "prisma:deploy:dev": "npx dotenv -e .env.migration.development -- prisma migrate dev",
    "prisma:deploy:prod": "npx dotenv -e .env.migration.production -- prisma migrate dev"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "pg-native": "^3.5.2",
    "rate-limit-redis": "^4.2.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ts-enum-util": "^4.1.0",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/luxon": "^3.7.1",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.1",
    "@types/pg": "^8.15.5",
    "@types/winston": "^2.4.4",
    "dotenv-cli": "^10.0.0",
    "prisma": "^6.15.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
